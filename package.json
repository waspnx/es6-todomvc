{
  "private": true,
  "dependencies": {
    "d3": "3.5.17",
    "lodash": "4.13.1",
    "rd3": "0.6.5",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "todomvc-app-css": "2.0.6",
    "todomvc-common": "1.0.2"
  },
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-preset-es2015-webpack": "6.4.2",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "chai": "3.5.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-config-kentcdodds": "^8.1.3",
    "ghooks": "1.3.2",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "istanbul": "0.4.4",
    "karma": "1.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-mocha": "1.1.1",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "npm-run-all": "2.3.0",
    "opt-cli": "1.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.19",
    "webpack-config-utils": "2.0.0",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.2.3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "postbuild": "cpy favicon.ico dist",
    "build": "webpack --env.prod -p",
    "build:dev": "webpack --env.dev",
    "start": "http-server dist",
    "dev": "webpack-dev-server --env.dev --hot",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "check-coverage": "istanbul check-coverage --statements 11 --branches 0 --functions 0 --lines 12",
    "validate": "npm-run-all --parallel lint build test --serial check-coverage",
    "lint": "eslint ."
  }
}
